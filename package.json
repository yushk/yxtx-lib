{
  "name": "yxtx-lib",
  "version": "1.1.2-1",
  "description": "vue2 components",
  "author": "yushk <945009587@qq.com>",
  "license": "MIT",
  "main": "lib/yxtx-lib.umd.js",
  "files": [
    "lib",
    "src",
    "types",
    "packages"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": " cross-env NODE_ENV=development webpack-dev-server  --config build/webpack.dev.js",
    "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js & node build/bin/i18n.js & node build/bin/version.js",
    "build:index": " node build/bin/build-entry.js",
    "build:demo": "npm run build:index && rimraf dist  && cross-env NODE_ENV=development webpack  --config build/webpack.dev.js",
    "lint": "eslint src/**/* test/**/* packages/**/* build/**/* --quiet",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:umd": "node build/bin/build-locale.js",
    "build:npm": "npm run clean && npm run build:index && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && npm run build:theme",
    "build:com": "webpack --config build/webpack.component.js "
  },
  "keywords": [
    "vue",
    "components lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yushk/vue2demo"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/component-compiler-utils": "^3.3.0",
    "@vue/composition-api": "^1.4.9",
    "@webpack-cli/generators": "^2.4.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^6.8.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "highlight.js": "^11.4.0",
    "html-webpack-plugin": "^5.5.0",
    "json-templater": "^1.2.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "throttle-debounce": "^1.1.0",
    "transliteration": "^1.6.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14",
    "vue-template-es2015-compiler": "^1.9.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "dependencies": {
    "echarts": "^5.3.0",
    "element-ui": "^2.15.6",
    "lodash": "^4.17.21",
    "vue-echarts": "^6.0.2",
    "vue-fragment": "^1.5.2",
    "vuex": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  }
}
